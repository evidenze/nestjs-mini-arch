{
  "name": "nestjs-mini-arch",
  "version": "1.0.0",
  "description": "A clean architectural framework for NestJS applications inspired by Laravel Lucid",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "mini": "dist/cli/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "nestjs",
    "architecture",
    "mini",
    "clean-architecture",
    "typescript",
    "framework",
    "microservices"
  ],
  "author": "Mini Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/evidenze/nestjs-mini-arch.git"
  },
  "bugs": {
    "url": "https://github.com/evidenze/nestjs-mini-arch/issues"
  },
  "homepage": "https://github.com/evidenze/nestjs-mini-arch#readme",
  "dependencies": {
    "@nestjs/common": "^9.0.0 || ^10.0.0",
    "@nestjs/core": "^9.0.0 || ^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "commander": "^14.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@nestjs/testing": "^11.1.5",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3"
  },
  "peerDependencies": {
    "@nestjs/common": "^9.0.0 || ^10.0.0",
    "@nestjs/core": "^9.0.0 || ^10.0.0"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ]
